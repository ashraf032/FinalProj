<section id="slider" data-aos="fade-up"
         data-aos-duration="3000">
    <div  class="slides row">
        <div class="slide-main col-12 m-0" >
            <div style="height:100%; " class="img ">
                <img style="height:27rem;"  src="~/assets/image/@Model.Sliders.FirstOrDefault().Image" alt="">
            </div>
        </div>
        @foreach (Slider slider in Model.Sliders)
        {
            <div class="slide" data-id="1">
                <div class="img ">
                    <img src="~/assets/image/@slider.Image.ToString()" alt="">
                    <p style="display: none;">@slider.SliderText.ToString()</p>
                </div>
            </div>
        }

        <div class="text  col-12">

            <div class="prews-dots" style="display:flex;justify-content:center">

                <div class="prews">
                    <div class="left-prew">
                        <svg xmlns="http://www.w3.org/2000/svg" class="HeroCarousel-module__caretIcon___ZySqK" viewBox="0 0 24 24"><path fill="none" d="M0 0h24v24H0z"></path><path fill="currentColor" d="M15.477 3.977a.503.503 0 01.365.841l-6.68 7.159 6.679 7.159a.502.502 0 01.159.366.5.5 0 01-.888.315l-7-7.5a.498.498 0 010-.682l7-7.5a.496.496 0 01.365-.16"></path></svg>
