<section id="tag">
    @if (Model.RestuorantsDeliveryFree != null)
    {
<h4>Something sweet 🍰🍫🍩</h4>
                <div class="owl-carousel owl-theme cards">
                    @await Html.PartialAsync("/Views/Shared/_HomePartial.cshtml", Model.RestaurantSweet)
                </div>
                                <h4>Free Delivery</h4>
                                                <div class="owl-carousel owl-theme cards">

                                                    @await Html.PartialAsync("/Views/Shared/_HomePartial.cshtml", Model.RestuorantsDeliveryFree)


                                                </div>}
    <h4>Discount</h4>
    <div class="owl-carousel owl-theme cards second-category">
        @await Html.PartialAsync("/Views/Shared/RestaurantPartialCampaign.cshtml", Model.RestuorantsCampaign)
    </div>

    <h4>Stores</h4>
    <div class="owl-carousel owl-theme cards">

        @await Html.PartialAsync("/Views/Shared/_StorePartial.cshtml", Model.Stores)



    </div>

</section>
